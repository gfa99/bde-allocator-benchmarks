#!/bin/bash

set -x  # Echo each command before executing it

time $1 -17 4 4 256 5 1
time $1 -17 4 4 256 5 0
time $1 -17 4 4 256 -5 1
time $1 -17 4 4 256 -5 0

time $1 -17 4 4 1 5 1
time $1 -17 4 4 1 5 0
time $1 -17 4 4 1 -5 1
time $1 -17 4 4 1 -5 0

time $1 -17 17 3 256 5 1
time $1 -17 17 3 256 5 0
time $1 -17 17 3 256 -5 1
time $1 -17 17 3 256 -5 0

time $1 -17 17 3 1 5 256
time $1 -17 17 3 1 5 0
time $1 -17 17 3 1 -5 256
time $1 -17 17 3 1 -5 0
