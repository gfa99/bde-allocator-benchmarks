#!/bin/bash

fgrep -v '*' copymove-result | grep . | (
  for cp in CP MV; do
    for arg1 in 04 17; do
      for arg2 in 1 2; do
        for arg3 in 1 2; do
          for arg4 in 0 1; do
            read time progname args
            read internals1
            read internals2
            read internals3
            read timename time
            read time2
            read time3
            # echo $time
            time=${time#0m}
            time=${time%s}
            echo $arg1$arg2$arg3$arg4$cp $args $time
          done
        done
      done
    done
  done
) | sort -n | (
  echo -n "Benchmark Arguments"
  echo -n ", use copies"
  echo    ", use moves"
  reftime=1.0
  while read key arg1 arg2 arg3 arg4 arg5 time; do
    case "$key" in
      (*CP) reftime=$time
            echo -n "$arg1 $arg2 $arg3 $arg4 $arg5, ${time}s"
            ;;
      (*)   reltime=`echo "scale=1; 100*$time/$reftime" | bc`
            printf ", %ss (%.0f%%)" "$time" "$reltime"
            ;;
    esac
    case "$key" in
      (*MV) echo ;;
    esac
  done
) > copymove.csv
